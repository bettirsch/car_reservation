<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.example.jedi.mapper.CarMapper">
	<resultMap id="carResult" type="com.example.jedi.mapper.model.Car">
		<id property="carId" jdbcType="NUMERIC" column="car_id" />
		<result property="name" jdbcType="VARCHAR" column="name" />
		<result property="plateNumber" jdbcType="VARCHAR" column="plate_number" />
		<result property="nrOfWheel" jdbcType="NUMERIC" column="nr_of_wheel" />
	</resultMap>
</mapper>
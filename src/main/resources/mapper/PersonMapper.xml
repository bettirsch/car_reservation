<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.example.jedi.mapper.PersonMapper">
	<resultMap id="personResult" type="com.example.jedi.model.Person">
		<id property="personId" column="person_id" />
		<result property="firstName" column="first_name" />
		<result property="lastName" column="last_name" />
		<collection property="cars" column="person_id" select="com.example.jedi.mapper.CarMapper.findReservedCarsByPerson"></collection>
	</resultMap>
</mapper>
